# msmtp configuration for relaying to host SMTP server
# Copy this file to msmtprc and customize if needed

defaults
auth off
tls off
logfile /var/log/msmtp.log

# Relay to host SMTP server
account default
host host.docker.internal
port 25
from asterisk@example.com

# If your host SMTP requires authentication, uncomment and configure:
# auth on
# user your-username
# password your-password
# tls on
# tls_starttls on
